{
  "name": "boujee-events",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Premium event management platform with Google Drive integration",
  "author": "Boujee Events Team",
  "license": "MIT",
  "homepage": "https://boujee-events.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/boujee-events.git"
  },
  "keywords": [
    "events",
    "management",
    "luxury",
    "premium",
    "react",
    "typescript",
    "google-drive",
    "supabase"
  ],
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "tsc && vite build",
    "preview": "vite preview --port 4173 --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "test-drive": "echo 'Testing Google Drive integration...' && npm run dev",
    "setup-env": "cp .env.example .env.local && echo 'Environment file created. Please fill in your API keys.'",
    "check-env": "node -e \"console.log('Environment check:'); console.log('VITE_GOOGLE_CLIENT_ID:', process.env.VITE_GOOGLE_CLIENT_ID ? '✅ Set' : '❌ Missing'); console.log('VITE_GOOGLE_DRIVE_API_KEY:', process.env.VITE_GOOGLE_DRIVE_API_KEY ? '✅ Set' : '❌ Missing');\"",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "@supabase/supabase-js": "^2.45.4",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "lucide-react": "^0.445.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.5.4",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.6.2",
    "vite": "^5.4.8",
    "terser": "^5.36.0",
    "@types/google.accounts": "^0.0.14",
    "vite-plugin-pwa": "^0.20.5",
    "workbox-window": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "overrides": {
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0"
  }
}
